.lh_0 {
  line-height: 0;
}
.esp {
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.esp_2 {
  width: 100%;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  word-break: break-all;
  overflow: hidden;
}

.esp_3 {
  width: 100%;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  word-break: break-all;
  overflow: hidden;
}

button {
  margin: 0;
  padding: 0;
  background-color: transparent;
  line-height: 1;
  text-align: left;
  border-radius: 0;
  border: 0;
}

