/* 空数据 */

@font-face {
  font-family: "no";
  src: url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2');
}

.not_found {
  position: relative;
  line-height: 300rpx;
  text-align: center;
  font-size: 150rpx;
  font-family: "no" !important;
  color: #999;
}

.not_found::before {
  content: "\e543";
}

.not_found::after {
  content: "暂无数据~";
  position: absolute;
  bottom: -110rpx;
  left: 50%;
  transform: translateX(-50%);
  font-size: 28rpx;
}

